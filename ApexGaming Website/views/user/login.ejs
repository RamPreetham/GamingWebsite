
<%- include('../partials/header.ejs')%>
<div id = "login-content">
    <h1>Login to Your Account!</h1>
    <form action="/nav/login" method="POST">
    <div class = "fields">
    <label for="email"></label>
    <input type="email" id="email"placeholder="Email" name="email" required> <br> <br>
    </div>
    <div class = "fields">
    <label for="password"></label>
    <input type="password" placeholder="Password" name="password" minlength="8" maxlength="64" required> <br> <br>
</div>
<div class = "fields">
    <button type="submit">Login</button> <br> 
</div>
<div class = "bottom">
    <p id = "dont"> Don't have an account?<a href="/nav/signup"> <b> Signup!</b></a> </p>
</div>
    </form>
</div>
